  "devDependencies": {
    "@szhou/script-tools": "^1.3.2",
    "@types/react": "^18.0.33",
    "@types/react-dev-utils": "^9.0.11",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.26",
    "@types/react-router-dom": "^5.1.6",
